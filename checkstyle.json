{
    "defaultSeverity": "INFO",
    "checks": [
        {
            "props": {},
            "type": "AvoidStarImport"
        },
        {
            "props": {},
            "type": "CommentedOutCode"
        },
        {
            "props": {
                "format": "^[A-Z][A-Z0-9]*(_[A-Z0-9]+)*$",
                "tokens": ["INLINE"]
            },
            "type": "ConstantName"
        },
        {
            "props": {
                "enforceEmptyPackage": true
            },
            "type": "EmptyPackage"
        },
        {
            "props": {
                "excludeNames": ["new", "toString", "main"],
                "fieldType": "BOTH",
                "ignoreOverride": true,
                "modifier": "PUBLIC",
                "requireParams": true,
                "requireReturn": true,
                "tokens": [
                    "ABSTRACT_DEF",
                    "CLASS_DEF",
                    "ENUM_DEF",
                    "INTERFACE_DEF"
                ]
            },
            "type": "FieldDocComment"
        },
        {
            "props": {
                "modifiers": [
                    "OVERRIDE",
                    "PUBLIC_PRIVATE",
                    "STATIC",
                    "EXTERN",
                    "MACRO",
                    "INLINE",
                    "DYNAMIC",
                    "FINAL"
                ]
            },
            "type": "ModifierOrder"
        },
        {
            "props": {},
            "type": "SimplifyBooleanExpression"
        },
        {
            "props": {},
            "type": "SimplifyBooleanReturn"
        },
        {
            "props": {
				"allowTrailingComma": false,
                "tokens": [",", ";"]
            },
            "type": "WhitespaceAfter"
        }
    ]
}
